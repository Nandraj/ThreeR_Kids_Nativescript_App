<Page>
    <ActionBar title="">
        <StackLayout orientation="horizontal">
            <Image src="~/images/3R2C/72.png" class="action-image"></Image>
            <Label text=" Kids" class="action-label-v2"></Label>
        </StackLayout>
    </ActionBar>
    <ScrollView>
        <GridLayout rows="auto, auto, auto, *">
            <Image row="0" src="~/images/3R2C/345.png" class="aboutImage" />
            <Label row="1" text="3R Coaching Classes" class="h1 text-center aboutHead"></Label>
            <GridLayout row="2" columns="*, *, *, *, *">
                <Button col="0" tap="doPhoneCall">
                    <FormattedString>
                        <Span text="&#xf879;" class="fas h2 aboutIcons"></Span>
                    </FormattedString>
                </Button>
                <Button col="1" tap="openWhatsapp">
                    <FormattedString>
                        <Span text="&#xf232;" class="fab h2 aboutIcons"></Span>
                    </FormattedString>
                </Button>
                <Button col="2" tap="sendEmail">
                    <FormattedString>
                        <Span text="&#xf0e0;" class="far h2 aboutIcons"></Span>
                    </FormattedString>
                </Button>
                <Button col="3" tap="openInstagram">
                    <FormattedString>
                        <Span text="&#xf16d;" class="fab h2 aboutIcons"></Span>
                    </FormattedString>
                </Button>
                <Button col="4" tap="openYtubeChannel">
                    <FormattedString>
                        <Span text="&#xf167;" class="fab h2 aboutIcons"></Span>
                    </FormattedString>
                </Button>
            </GridLayout>
            <Label row="3" text=""></Label>
        </GridLayout>
    </ScrollView>
</Page>